{
    "license": "MIT",
    "version": "1.0.0",
    "private": false,
    "name": "robo-nouns-contracts",
    "description": "RoboNouns VRGDA EVM Contracts",
    "scripts": {
        "node": "npx hardhat node --hostname 0.0.0.0",
        "start": "hardhat node",
        "clean": "hardhat clean",
        "fetch-local": "npx hardhat fetch --network localhost",
        "fetch-goerli": "npx hardhat fetch --network goerli",
        "deploy-local": "npx hardhat deploy-goerli --network localhost",
        "deploy-goerli": "npx hardhat deploy-goerli --network goerli",
        "descriptor-local": "npx hardhat populate-descriptor --network localhost",
        "descriptor-goerli": "npx hardhat populate-descriptor --network goerli",
        "build": "yarn build:sol --force && yarn build:ts && yarn build:declarations",
        "build:ts": "tsc -p tsconfig.build.json",
        "build:sol": "npx hardhat compile",
        "build:declarations": "shx cp typechain/*.d.ts dist/typechain",
        "connect-local": "npx hardhat console --network localhost"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "hardhat-abi-exporter": "^2.10.1",
        "prompt": "^1.3.0"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.2",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.3",
        "@nomiclabs/hardhat-ethers": "^2.2.3",
        "@nomiclabs/hardhat-etherscan": "^3.1.0",
        "@openzeppelin/contracts": "^4.7.1",
        "@openzeppelin/contracts-upgradeable": "^4.8.2",
        "@remix-project/remixd": "^0.6.4",
        "@tenderly/hardhat-tenderly": "^1.1.4",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/chai": "^4.3.1",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^9.1.1",
        "@types/node": "^16.0.0",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "VRGDAs": "https://github.com/transmissions11/VRGDAs.git#3f35c0b6e2997fee1eb61e436e390d3d9470288b",
        "base64-sol": "^1.0.1",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "cspell": "^6.4.1",
        "dotenv": "^16.0.1",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "ethers": "^5.6.9",
        "hardhat": "^2.10.1",
        "hardhat-contract-sizer": "^2.6.1",
        "hardhat-docgen": "^1.3.0",
        "hardhat-finder": "^2.0.0",
        "hardhat-gas-reporter": "^1.0.8",
        "hardhat-spdx-license-identifier": "^2.0.3",
        "hardhat-storage-layout": "^0.1.6",
        "hardhat-tracer": "1.1.0-rc.6",
        "istanbul-badges-readme": "^1.8.2",
        "prettier": "^2.7.1",
        "prettier-plugin-solidity": "1.0.0-dev.23",
        "shellcheck": "^1.1.0",
        "shx": "^0.3.3",
        "solhint": "^3.3.7",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.21",
        "solmate": "^6.7.0",
